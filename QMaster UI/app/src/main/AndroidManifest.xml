<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.barnes.ummoqmasterdashboard" >
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeDrawer"
            android:label="@string/app_name"
            android:theme="@style/MaterialDrawerTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ManageQDrawer"
            android:label="@string/app_name"
            android:theme="@style/MaterialDrawerTheme.Light.TranslucentStatus" />
        <activity android:name=".QCreateQForm"
            android:parentActivityName=".HomeDrawer" >
            <!-- The meta-data element is needed for versions lower than 4.1 -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeDrawer" /></activity>
        <activity
            android:name=".RecyclerViewActivity"
            android:label="@string/app_name"
            android:theme="@style/MaterialDrawerTheme.Light.TranslucentStatus" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/MaterialDrawerTheme.Light.TranslucentStatus" />
    </application>
</manifest>
