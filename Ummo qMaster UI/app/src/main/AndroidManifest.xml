<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ummo.blinker.ummoqmasternavsample" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actions.CreateQ"
            android:label="@string/title_activity_create_q" >
        </activity>
        <activity
            android:name=".actions.ActivateQ"
            android:label="@string/title_activity_activate_q" >
        </activity>
        <activity
            android:name=".actions.ReviewQ"
            android:label="@string/title_activity_review_q" >
        </activity>
        <activity
            android:name=".actions.DialogActivity"
            android:theme="@style/AppTheme.CustomTheme"
            android:label="Dialog" >
        </activity>
    </application>

</manifest>
