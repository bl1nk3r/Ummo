<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ummo.blinker.ummoqmasternavsample.MyQFragment"
    android:background="#dedede">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="My Queue"
            android:id="@+id/myQTV"
            android:layout_gravity="center"
            android:layout_marginTop="37dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="68dp"
            android:layout_marginStart="68dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text=""
            android:id="@+id/myQViewTV"
            android:layout_marginLeft="36dp"
            android:layout_marginStart="36dp"
            android:layout_alignBottom="@+id/myQTV"
            android:layout_toRightOf="@+id/myQTV"
            android:layout_toEndOf="@+id/myQTV" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Q Active:"
            android:id="@+id/textView3"
            android:layout_below="@+id/myQTV"
            android:layout_alignLeft="@+id/myQTV"
            android:layout_alignStart="@+id/myQTV"
            android:layout_marginTop="91dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="is it?"
            android:id="@+id/qActiveTV"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignLeft="@+id/myQViewTV"
            android:layout_alignStart="@+id/myQViewTV" />

    </RelativeLayout>

</FrameLayout>
